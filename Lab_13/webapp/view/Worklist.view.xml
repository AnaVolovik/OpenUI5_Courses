<mvc:View
  controllerName="Lab_13.Lab_13.controller.Worklist"
  xmlns="sap.m"
  xmlns:f="sap.f"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:table="sap.ui.table"
  xmlns:s006="jblibs.s006">

  <f:DynamicPage>
    <f:header>
      <f:DynamicPageHeader pinnable="false">
        
      </f:DynamicPageHeader>
    </f:header>

    <f:content>
      <NavContainer height="100%" width="100%">
        <table:Table
          id="table"
          rows="{/zjblessons_base_Items}">

          <table:columns>
            <table:Column>
              <Label text="{i18n>HeaderID}"/>
              <table:template>
                <Text text="{HeaderID}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>MaterialID}"/>
              <table:template>
                <Text text="{MaterialID}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>GroupID}"/>
              <table:template>
                <Text text="{GroupID}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>SubGroupID}"/>
              <table:template>
                <Text text="{SubGroupID}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>Quantity}"/>
              <table:template>
                <Text text="{Quantity}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>Price}"/>
              <table:template>
                <Text text="{Price}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>Created}"/>
              <table:template>
                <Text text="{Created}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>CreatedBy}"/>
              <table:template>
                <Text text="{CreatedBy}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>Modified}"/>
              <table:template>
                <Text text="{Modified}" />
              </table:template>
            </table:Column>
            <table:Column>
              <Label text="{i18n>ModifiedBy}"/>
              <table:template>
                <Text text="{ModifiedBy}" />
              </table:template>
            </table:Column>
          </table:columns>
        </table:Table>
      </NavContainer>
    </f:content>
  </f:DynamicPage>

</mvc:View>